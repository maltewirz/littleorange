{
    "name": "nodetemplate",
    "version": "0.9.0",
    "description": "Template for React Node",
    "engines": {
        "node": "10.x"
    },
    "scripts": {
        "start": "node server",
        "build": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
        "startup": "concurrently \"nodemon server\" \"cd react-ui/ && npm start\""
    },
    "cacheDirectories": [
        "node_modules",
        "react-ui/node_modules"
    ],
    "dependencies": {
        "axios": "^0.19.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "concurrently": "^4.1.0",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^1.3.3",
        "csurf": "^1.10.0",
        "express": "^4.17.1",
        "spiced-pg": "^1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maltewirz/littleorange.git"
    },
    "devDependencies": {}
}
